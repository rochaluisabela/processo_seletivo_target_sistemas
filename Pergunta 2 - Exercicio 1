var1 = 0
var2 = 1
var3 = 0


while var3 <= num:
  print("Consulta Sequeência de Fibonacci")
  num = int(input("Entre com um número: ")) 
  var3 = var1 + var2
  var1 = var2
  var3 = var1 + var2
  if num == 0 or num == 1:
     print("O número pertence a sequência Fibonacci")
  elif num == var3:
       print("O número pertence a sequência Fibonacci")
  else:
    print("O número não pertence a sequência Fibonacci")

